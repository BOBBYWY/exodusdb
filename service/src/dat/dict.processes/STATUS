#include <processlocking.h>\
\
var islocked;\
\
function main() {\
	// NB add any new statuses to MONITORING program\
	if (RECORD.f(52)) {\
		ANS = "Closed";\
		return ANS;\
	}\
	call processlocking("ISLOCKED", ID, islocked);\
	// if (not(islocked) and VOLUMES) {\
	//	ANS = "Crashed";\
	//	return ANS;\
	//}\
\
	let dostimenow = date() + 24873 + time() / 86400;\
	// 10 mins\
	let hungtime = 10 * 60 / 86400.0;\
	let r33 = RECORD.f(33);\
	if (r33 and (dostimenow - RECORD.f(27) gt hungtime)) {\
		// if (VOLUMES) {\
		//	ANS = "Hung";\
		//} else {\
			ANS = "Closed";\
		//}\
		return ANS;\
	}\
	if (r33) {\
		ANS = "OK";\
	} else {\
		ANS = "Maintenance";\
	}\
	if (RECORD.f(53)) {\
		ANS ^= " " ^ RECORD.f(53);\
	}\
	return ANS;\
}
