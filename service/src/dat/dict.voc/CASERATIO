MD00P
	var xx = RECORD;\
	xx.converter(LOWERCASE, "");\
	var yy = RECORD;\
	yy.converter(UPPERCASE, "");\
	var uu = RECORD.length() - yy.length();\
	var ll = RECORD.length() - xx.length();\
	if (uu gt ll) {\
		ANS = uu / (ll + 1);\
	} else {\
		ANS = -(ll / (uu + 1));\
	}\
	return ANS;
