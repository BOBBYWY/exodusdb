#include <sys_common.h>\
\
function main() {\
	#include <system_common.h>\
	var ans = field2(RECORD, FM, -1);\
	if (ans[1] eq "V") {\
		ans = field2(RECORD, FM, -2);\
	}\
	if (not(ans.substr(1, 2).match("^\\d{2}$"))) {\
		ANS = "";\
	} else {\
		ANS = ans;\
		}\
	return ANS;\
}
