#!/bin/bash

#set -x
#set -e

echo --------------Generate all headers--------------
compile ../???/*.cpp {HS}

for MODULE in ext add sys gen fin agy med job tim
do
        test -d ../$MODULE || continue

#       cp ~/arev/$MODULE/*.cpp ../$MODULE/
        echo --------------Compile $MODULE ------------------
        compile ../$MODULE/*.cpp \($1\)
done

