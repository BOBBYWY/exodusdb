#!/bin/bash

#set -x
#set -e

echo --------------Generate all headers--------------
compile ../???/*.cpp {HS}

for MODULE in add sys gen agy job tim
do
#	cp ~/arev/$MODULE/*.cpp ../$MODULE/
	echo --------------Compile $MODULE ------------------
	compile ../$MODULE/*.cpp \($1\)
done

compile ../fin/initacc ../fin/readaccparams  \($1\)
